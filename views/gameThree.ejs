<%- include header %>
<%- include top %>
<div class="wrap">
    <div class="warp_game">
        <div class="square_remote" id="remote">
            <div class="remote_game" id="remote_game"></div>
        </div>
        <div class="square_Next">
            <div class="next_title">N E X T</div>
            <div class="next">
                <div class="remote_next" id="remote_next"></div>
            </div>
            <div class='remote_gameover' id="remote_gameover"></div>
            <!-- <div id='waiting'></div> -->
        </div>
        <div class="square_local" id="local">
            <div class="local_game" id="local_game"></div>
            <!-- <div id="local_gameover"></div> -->
        </div>
        <div class="square_next">
            <div class="next_title">N E X T</div>
            <div class="next">
                <div class="local_next" id="local_next"></div>
            </div>
            <ul class="local_data">
                <li>当前速度<br /><span class="number" id='local_speed'>1</span></li>
                <li>已用时间<br /><span class="number" id="local_time"> 00:00:00 </span></li>
            </ul>
            <div class='local_gameover' id="local_gameover"></div>

        </div>
        <div class='local_music'>
            <span class="music" id="music"></span>
            <!-- <span class="restart" id="restart"></span> -->
        </div>
    </div>

</div>
<script src="/javascripts/square.js"></script>
<script src="/javascripts/squareFactory.js"></script>
<script src="/javascripts/game.js"></script>
<script src="/javascripts/threeLocal.js"></script>
<script src="/javascripts/twoRemote.js"></script>
<script src="/javascripts/threeScript.js"></script>
</body>

</html>